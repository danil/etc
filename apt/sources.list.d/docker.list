# Docker <https://docs.docker.com/engine/install/ubuntu/#install-using-the-repository>:
# sudo aptitude remove docker docker-engine docker.io containerd runc
# sudo aptitude install apt-transport-https ca-certificates curl gnupg gnupg-agent lsb-release software-properties-common
# curl -fsSL https://download.docker.com/linux/ubuntu/gpg | sudo gpg --dearmor -o /usr/share/keyrings/docker-archive-keyring.gpg
# and then:
# sudo aptitude update
# sudo aptitude install docker-ce docker-ce-cli containerd.io
deb [arch=amd64 signed-by=/usr/share/keyrings/docker-archive-keyring.gpg] https://download.docker.com/linux/ubuntu impish stable
